<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <domain-config cleartextTrafficPermitted="true">

        <domain includeSubdomains="true">https://indianrider.co.in/</domain>
        <domain includeSubdomains="true">http://indianrider.co.in/</domain>
        <domain includeSubdomains="true">https://fcm.googleapis.com</domain>
    </domain-config>
    <base-config cleartextTrafficPermitted="true">
        <trust-anchors>
            <certificates src="system" />
        </trust-anchors>
    </base-config>
</network-security-config>